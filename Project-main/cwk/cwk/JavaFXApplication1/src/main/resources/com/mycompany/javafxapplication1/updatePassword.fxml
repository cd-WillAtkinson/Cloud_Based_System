<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.javafxapplication1.UpdatePasswordController">
    <Label text="Update Password" style="-fx-font-size: 18px;"/>
    
    <Label text="New Password:"/>
    <PasswordField fx:id="passwordField" promptText="Enter new password"/>
    
    <Button text="Save Changes" onAction="#handleSaveChanges"/>
    <Button text="Cancel" onAction="#handleCancel"/>
</VBox>
